   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch bot",
         "program": "${workspaceFolder}/dist/main/index.js",
         "outputCapture": "std",
         "sourceMaps": true,
         "cwd": "${workspaceFolder}",
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Run tests",
         "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
         "outputCapture": "std",
         "sourceMaps": true,
         "args": [
            "--config=./jasmine.json"
         ],
      },
   ]
}
